    "build:decisions": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=PostedDecisions --env SITE=PostedDecisions --config webpack/webpack.dev.config",
    "build:ceu": "webpack --env SITE=CeuIntake --config webpack/webpack.dev.config",
    "build:admin:staging": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Admin --env SITE=admin --config webpack/webpack.staging.config",
    "build:intake:staging": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Intake --env SITE=intake --config webpack/webpack.staging.config",
    "build:intake2:staging": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=AdditionalLandlordIntake --env SITE=AdditionalLandlordIntake --config webpack/webpack.staging.config",
    "build:evidence:staging": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=DisputeAccess --env SITE=evidence --config webpack/webpack.staging.config",
