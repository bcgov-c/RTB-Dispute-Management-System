    "build:ceu:staging": "webpack --env SITE=CeuIntake --config webpack/webpack.staging.config",
    "build:admin:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Admin --env SITE=admin --config webpack/webpack.prod.config",
    "build:intake:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Intake --env SITE=intake --config webpack/webpack.prod.config",
    "build:intake2:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=AdditionalLandlordIntake --env SITE=AdditionalLandlordIntake --config webpack/webpack.prod.config",
    "build:evidence:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=DisputeAccess --env SITE=evidence --config webpack/webpack.prod.config",
    "build:office:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=OfficeSubmission --env SITE=office --config webpack/webpack.prod.config",
    "build:decisions:prod": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=PostedDecisions --env SITE=PostedDecisions --config webpack/webpack.prod.config",
    "build:ceu:prod": "webpack --env SITE=CeuIntake --config webpack/webpack.prod.config",
    "build:adminintake": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=AdminIntake --env SITE=intake --config webpack/webpack.prod.config",
    "build:dev": "yarn build:admin && yarn build:intake && yarn build:intake2 && yarn build:evidence && yarn build:office &&  yarn build:decisions",
    "build:staging": "yarn build:admin:staging && yarn build:intake:staging && yarn build:intake2:staging && yarn build:evidence:staging && yarn build:office:staging && yarn build:decisions:staging && yarn build:ceu:staging",
    "build:preprod": "yarn build:admin:prod --env preprod && yarn build:intake:prod --env preprod && yarn build:intake2:prod --env preprod && yarn build:evidence:prod --env preprod && yarn build:office:prod --env preprod && yarn build:decisions:prod --env preprod && yarn build:ceu:prod --env preprod",
    "build:prod": "yarn build:admin:prod && yarn build:intake:prod && yarn build:intake2:prod && yarn build:evidence:prod && yarn build:office:prod && yarn build:decisions:prod && yarn build:ceu:prod",
    "build:openshift": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Admin --env SITE=admin --config webpack/webpack.dev.config && webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Intake --env SITE=intake --config webpack/webpack.dev.config && webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=DisputeAccess --env SITE=evidence --config webpack/webpack.dev.config && webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=OfficeSubmission --env SITE=office --config webpack/webpack.dev.config"
  },
