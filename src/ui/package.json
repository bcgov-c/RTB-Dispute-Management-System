  "docs": "jsdoc ./src -r -c ./jsdoc.conf -d ./docs",
  "scripts": {
    "test": "jest",
    "coverage": "yarn test -- --coverage",
    "lint": "eslint src",
    "dev:intake": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=intake --env APP_PORT=3000",
    "dev:intake2": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=AdditionalLandlordIntake --env APP_PORT=3004",
    "dev:ceu": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=CeuIntake --env APP_PORT=3000",
    "dev:admin": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=admin --env APP_PORT=3001",
    "dev:evidence": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=evidence --env APP_PORT=3002",
    "dev:office": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=office --env APP_PORT=3003",
    "dev:decisions": "webpack-dev-server --config webpack/webpack.dev.config --env SITE=PostedDecisions --env APP_PORT=3005",
    "build:admin": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Admin --env SITE=admin --config webpack/webpack.dev.config",
    "build:intake": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=Intake --env SITE=intake --config webpack/webpack.dev.config",
    "build:intake2": "webpack --env RELATIVE_OUTPUT_DIR_OVERRIDE=AdditionalLandlordIntake --env SITE=AdditionalLandlordIntake --config webpack/webpack.dev.config",
